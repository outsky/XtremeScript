/* c style single line comment */
func MyFunc ( Param0, Param1, Param2 )
{
	return ( Param0 + Param1 ) * Param2;
}

/*
c style muti line comment
*/
func main ()
{
    // cpp style comment
	var MyString;
	var X;
	
	MyString = "This is a \"real\" XtremeScript script!";
	X = 256;
	
	MyFunc ( MyString, 3.14159, X );
}
