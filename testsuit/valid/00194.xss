host print();
host println();

func main()
{
   var a;
   var b;

   a = 0;
   while (a < 2)
   {
      print(a++);
      break;

      b = "A";
      while (b < "C")
      {
         print(b++);
      }
      print("e");
   }
   println("");

   return 0;
}

